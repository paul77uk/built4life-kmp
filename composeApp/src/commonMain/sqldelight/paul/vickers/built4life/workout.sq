CREATE TABLE workout (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    weight INTEGER,
    eliteLevel INTEGER,
    creationDate TEXT DEFAULT (datetime('now')) NOT NULL
);

getAll:
SELECT * FROM workout;

getById:
SELECT * FROM workout WHERE id = ?;

upsert:
INSERT OR REPLACE INTO workout(id, title, weight, eliteLevel) VALUES(?, ?, ?, ?);

delete:
DELETE FROM workout WHERE id = ?;

getAllWorkoutsWithScores:
SELECT workout.id, workout.title, workout.eliteLevel, workout.weight, score.id, score.reps, score.creationDate FROM workout LEFT JOIN score ON workout.id = score.workoutId ORDER BY workout.creationDate;